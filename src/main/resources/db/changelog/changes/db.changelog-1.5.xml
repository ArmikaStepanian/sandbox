<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <changeSet id="20200404-1-2" author="stepanian">
        <createTable tableName="measure" remarks="Measure">
            <column name="id" type="INTEGER" remarks="Entry id" autoIncrement="true">
                <constraints nullable="false" primaryKey="true" primaryKeyName="PK_MEASURE_ID"/>
            </column>
            <column name="title" type="VARCHAR(255)" remarks="Measure title">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>
